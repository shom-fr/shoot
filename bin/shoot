#!/usr/bin/env python3
# -*- coding: utf-8 -*-
"""
shoot commandline interface
"""
import sys
import pathlib

this_file = pathlib.Path(__file__)
if (this_file.parent.parent / "shoot" / "__init__.py").exists():
    sys.path.insert(0, this_file.parent.parent)


from shoot.cli import main

if __name__ == "__main__":
    main()
